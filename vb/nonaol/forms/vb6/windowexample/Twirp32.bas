Public Sub Timeout(Duration As Long)
    Dim Now As Long
    Now = Timer
    Do Until Timer - Now >= Duration
        DoEvents
    Loop
End Sub
Sub Playwav(FilePath As String)
    Dim SoundName As String, Play, Flags As Integer
    SoundName$ = FilePath$
    Flags% = SND_ASYNC Or SND_NODEFAULT
    Play = sndPlaySound(SoundName$, Flags%)
End Sub
Sub WinSetText(theWindow As Long, theText As String)
Call SendMessageByString(theWindow&, WM_SETTEXT, 0, theText)
End Sub

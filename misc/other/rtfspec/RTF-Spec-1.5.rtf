\par //
\par // %%Function: ecApplyPropChange
\par //
\par // Set the property identified by _iprop_ to the value _val_.
\par //
\par //
\par 
\par int
\par ecApplyPropChange(IPROP iprop, int val)
\par \{
\par     char *pb;
\par 
\par     if (rds == rdsSkip)                 // If we're skipping text,
\par         return ecOK;                    // don't do anything.
\par 
\par     switch (rgprop[iprop].prop)
\par     \{
\par     case propDop:
\par         pb = (char *)&dop;
\par         break;
\par     case propSep:
\par         pb = (char *)&sep;
\par         break;
\par     case propPap:
\par         pb = (char *)&pap;
\par         break;
\par     case propChp:
\par         pb = (char *)&chp;
\par         break;
\par     default:
\par         if (rgprop[iprop].actn != actnSpec)
\par             return ecBadTable;
\par         break;
\par     \}
\par     switch (rgprop[iprop].actn)
\par     \{
\par     case actnByte:
\par         pb[rgprop[iprop].offset] = (unsigned char) val;
\par         break;
\par     case actnWord:
\par         (*(int *) (pb+rgprop[iprop].offset)) = val;
\par         break;
\par     case actnSpec:
\par         return ecParseSpecialProperty(iprop, val);
\par         break;
\par     default:
\par         return ecBadTable;
\par     \}
\par     return ecOK;
\par \}
\par 
\par //
\par // %%Function: ecParseSpecialProperty
\par //
\par // Set a property that requires code to evaluate.
\par //
\par 
\par int
\par ecParseSpecialProperty(IPROP iprop, int val)
\par \{
\par     switch (iprop)
\par     \{
\par     case ipropPard:
\par         memset(&pap, 0, sizeof(pap));
\par         return ecOK;
\par     case ipropPlain:
\par         memset(&chp, 0, sizeof(chp));
\par         return ecOK;
\par     case ipropSectd:
\par         memset(&sep, 0, sizeof(sep));
\par         return ecOK;
\par     default:
\par         return ecBadTable;
\par     \}
\par     return ecBadTable;
\par \}
\par 
\par //
\par // %%Function: ecTranslateKeyword.
\par //
\par // Step 3.
\par // Search rgsymRtf for szKeyword and evaluate it appropriately.
\par //
\par // Inputs:
\par // szKeyword:   The RTF control to evaluate.
\par // param:       The parameter of the RTF control.
\par // fParam:      fTrue if the control had a parameter; (that is, if param is valid)
\par //              fFalse if it did not.
\par //
\par 
\par int
\par ecTranslateKeyword(char *szKeyword, int param, bool fParam)
\par \{
\par     int isym;
\par 
\par     // search for szKeyword in rgsymRtf
\par 
\par     for (isym = 0; isym < isymMax; isym++)
\par         if (strcmp(szKeyword, rgsymRtf[isym].szKeyword) == 0)
\par             break;
\par     if (isym == isymMax)            // control word not found
\par     \{
\par         if (fSkipDestIfUnk)         // if this is a new destination
\par             rds = rdsSkip;          // skip the destination
\par                                     // else just discard it
\par         fSkipDestIfUnk = fFalse;
\par         return ecOK;
\par     \}
\par 
\par     // found it!  use kwd and idx to determine what to do with it.
\par 
\par     fSkipDestIfUnk = fFalse;
\par     switch (rgsymRtf[isym].kwd)
\par     \{
\par     case kwdProp:
\par         if (rgsymRtf[isym].fPassDflt || !fParam)
\par             param = rgsymRtf[isym].dflt;
\par         return ecApplyPropChange(rgsymRtf[isym].idx, param);
\par     case kwdChar:
\par         return ecParseChar(rgsymRtf[isym].idx);
\par     case kwdDest:
\par         return ecChangeDest(rgsymRtf[isym].idx);
\par     case kwdSpec:
\par         return ecParseSpecialKeyword(rgsymRtf[isym].idx);
\par     default:
\par         return ecBadTable;
\par     \}
\par     return ecBadTable;
\par \}
\par 
\par //
\par // %%Function: ecChangeDest
\par //
\par // Change to the destination specified by idest.
\par // There's usually more to do here than this...
\par //
\par 
\par int
\par ecChangeDest(IDEST idest)
\par \{
\par     if (rds == rdsSkip)             // if we're skipping text,
\par         return ecOK;                // don't do anything
\par 
\par     switch (idest)
\par     \{
\par     default:
\par         rds = rdsSkip;              // when in doubt, skip it...
\par         break;
\par     \}
\par     return ecOK;
\par \}
\par 
\par //
\par // %%Function: ecEndGroupAction
\par //
\par // The destination specified by rds is coming to a close.
\par // If there's any cleanup that needs to be done, do it now.
\par //
\par 
\par int
\par ecEndGroupAction(RDS rds)
\par \{
\par     return ecOK;
\par \}
\par 
\par //
\par // %%Function: ecParseSpecialKeyword
\par //
\par // Evaluate an RTF control that needs special processing.
\par //
\par 
\par int
\par ecParseSpecialKeyword(IPFN ipfn)
\par \{
\par     if (rds == rdsSkip && ipfn != ipfnBin)  // if we're skipping, and it's not
\par         return ecOK;                        // the \\bin keyword, ignore it.
\par     switch (ipfn)
\par     \{
\par     case ipfnBin:
\par         ris = risBin;
\par         cbBin = lParam;
\par         break;
\par     case ipfnSkipDest:
\par         fSkipDestIfUnk = fTrue;
\par         break;
\par     case ipfnHex:
\par  ris = risHex;
\par  break;
\par     default:
\par         return ecBadTable;
\par     \}
\par     return ecOK;
\par \}

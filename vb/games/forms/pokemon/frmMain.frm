Private Sub cmdGenerate_Click()
    txtBuffer.Text = EncryptA(txt1.Text) + vbNewLine + EncryptA(txt2.Text) + vbNewLine + EncryptA(txt3.Text) + vbNewLine + EncryptA(txt4.Text) + vbNewLine + EncryptA(txt5.Text) + vbNewLine + EncryptA(txt6.Text) + vbNewLine + txt7.Text + vbNewLine + EncryptA(txt8.Text)
    If LineCount(txtBuffer.Text) > 0 Then
        CD1.ShowSave
    Else
        If MsgBox("License may not be complete, continue?", vbYesNo + vbQuestion) = vbYes Then
            CD1.ShowSave
        End If
    End If
    If Not CD1.filename = Empty Then
        SaveText txtBuffer.Text, CD1.filename
    End If
End Sub
